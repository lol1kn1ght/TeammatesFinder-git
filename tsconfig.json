{
  "compilerOptions": {
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "./resources",
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "strict": true
  }
}
